import api from 'src/api'

const state = {
  send: []
}

const getters = {
  sendData: state => state.send   // 发信设置
}

const mutations = {
  SEND_DATA: (state, data) => {
    console.log('服务端发信设置结果', data)
    state.send = data.data
  }
}

const actions = {
  isSend ({state, commit}, parameter) {
    console.log('开始获取---发信设置')
    return api.post('/v1/mail/detail', parameter).then((res) => {
      commit('SEND_DATA', res)
    }).catch((error) => {
      console.log(error)
    })
  }
}

export default {
  state,
  getters,
  mutations,
  actions
}
