<template>
  <div>
    <win-header></win-header>
    <router-view></router-view>
  </div>
</template>
<script lang="babel">
// import { mapGetters } from 'vuex'
import WinHeader from 'components/header/Header'
export default {
  data () {
    return { title: '' }
  },
  mounted () { },
  components: {
    WinHeader
  },
  watch: {
    '$route' (to, from) {
      switch (to.path) {
        case '/':
          document.title = '首页'
          break
        case '/user':
          document.title = '用户'
          break
        case '/citys':
          document.title = '地区'
          break
        case '/about':
          document.title = '我的'
          break
        case '/login':
          document.title = '登录'
          break
        default:
          document.title = 'vue-ssr'
      }
    }
  }
}

</script>
